
@{
    ViewBag.Title = "OrderIndex";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}
<div class="page-content-wrapper">
    <div class="row">
        <div class="col-12">
            <div class="card m-b-20">
                <div class="card-body">

                    <h4 class="mt-0 header-title">Examples</h4>
                    <p class="text-muted m-b-30">just start typing to edit, or move around with arrow keys or mouse clicks!</p>

            <div id="pageOrder">

            </div>

                </div>
            </div>
        </div> <!-- end col -->
    </div> <!-- end row -->
</div>
<script>

    $(document).ready(function () {
        GETCart();
    })

    function GETCart() {
        $.ajax({
            type: 'Get',
            url: '/Order/OrderGet',
            success: function (res) {
                $('#pageOrder').html(res);
            }
        })
    }
</script>





